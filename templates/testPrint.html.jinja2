<html>
    <head>
        <title>Debug page</title>
    </head>

    <body>
    <p>DATABASE DISPLAY :</p>
    <div>
    <p>ORGANISATIONS :</p>
        {% for orga in organisations %}
            <p> Organisation : {{orga.name}} (id={{orga.id}})
            liste des personnes dans cette entit√© : {{ orga.personnes }}</p>
        {% endfor %}
    <br><br>
    </div>
    <div>
    <br>
        <p>POSITIONS :</p>
        {% for p in positions %}
            <p>  {{p.titre}}(id={{p.id}}), organisations =

                {{ p.organisations }}</p>
        {% endfor %}
    <br><br>
    </div>

        <p>PFEs : </p>
        {% for pfe in pfes %}
            <p>projet {{ pfe.titre }} : {{ pfe.description }}</p>
        {% endfor %}
    </div>
    <div>
        <p>TAFS : </p>
        {% for taf in tafs %}
             {{ taf.name }},
        {% endfor %}
    </div>
    <div>
        <p>PERSONNES : </p>
        {% for t in personnes %}
             {{ t.name }},
        {% endfor %}
    <br><br>
    </div>
    <div>
        <p>LIEN POSITIONS/PERSONNES <br></p>
        {% for p in positions %}
             {{ p.titre }} : {{ p.personnes }},
        {% endfor %}
    </div>
    <div>
        <p>TEST LIEN PERSONNES/TAFS : </p>
        A partir des tafs :
        {% for t in tafs %}
             Personnes dans {{ t.name }} : {% for el in t.personnes %} {{ el.name }} {% endfor %} <br>
        {% endfor %} <br><br>
        A partir des personnes : <br>
        {% for p in personnes %}
            {{ p.name }} est en {{ p.taf }} <br>
        {% endfor %}
    </div>


       <div>
        <p>PFES DES ALUMNIS: </p>
       Liste des alumnis : {{ alumnis }}

    </div>


    </body>
</html>